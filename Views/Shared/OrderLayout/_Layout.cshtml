    </div>
<!-- Pre-loader end -->
<div id="pcoded" class="pcoded">
    <div class="pcoded-overlay-box"></div>
    <div class="pcoded-container navbar-wrapper main-page">
        <!--header-->    
        @{await Html.RenderPartialAsync("OrderLayout/_Header");}
        <!--end header-->
        @RenderSection("Ribbon", required: false)
        @if (!User.Identity.IsAuthenticated)
        {
            Layout = "AuthLayout/_AuthorizePage";
            <div class="">
                @RenderBody()
            </div>
        }
        else
        {
            <div class="pcoded-main-container main-page">
                <div class="pcoded-wrapper">
                    <div class="">
                        @RenderBody()
                    </div>
                </div>
                <!-- Main-body end -->
                @{await Html.RenderPartialAsync("Layout/_WarningPage");}
            </div>
        }
    </div>
</div>

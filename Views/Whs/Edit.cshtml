@{
    Layout = "InvLayout/_InvLayout";
}
@model Whs
<!-- Page-header start -->
<div class="page-header ribbon-header">
    <div class="row align-items-end">
        <div class="col-lg-8">
            <div class="page-header-title">
                <div class="d-inline">
                    <h4>Warehouse</h4>
                    <span>Edit Warehouse</span>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            <div class="page-header-breadcrumb">
                <ul class="breadcrumb-title">
                    <li class="breadcrumb-item">
                        <a asp-action="index" asp-controller="Home"> <i class="icofont icofont-home"></i> </a>
                    </li>
                    <li class="breadcrumb-item">
                        <a>Warehouse</a>
                    </li>
                     <li class="breadcrumb-item">
                        <a href="#!">Edit</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
<!-- Page-header end -->

<div class="row">
    <div class="col-sm-12">
        <!-- Basic Form Inputs card start -->
        <div class="card">
            <div class="card-block">
                <h4 class="sub-title" style="border-bottom:2px #00ACED solid ">Warehouse Form</h4>
                <form asp-action="Edit" asp-controller="Whs" role="form" method="post" enctype="multipart/form-data">
                    <input type="hidden" asp-for="Id"/>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" asp-for="Whs_En">Whs Name <span class="text-danger">*</span></label>
                        <div class="col-sm-10">
                            <input type="text" asp-for="Whs_En" class="form-control" required="required">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" asp-for="Whs_Str">Whs Foriegn</label>
                        <div class="col-sm-10">
                            <input type="text" asp-for="Whs_Str" class="form-control">
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" asp-for="Address">Address <span class="text-danger">*</span></label>
                        <div class="col-sm-10">
                            <textarea type="text" class="form-control" asp-for="Address"></textarea>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label" asp-for="Remark">Remark </label>
                        <div class="col-sm-10">
                            <textarea type="text" asp-for="Remark" class="form-control">></textarea>
                        </div>
                    </div>
                    <div class="form-group text-right">
                        <button type="submit" class="btn btn-success">Update</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
